</head>
<body>
    <!-- header -->
    <div class="banner banner2">
        <div class="container">
            <div class="headr-right">
            </div>
            <div class="banner_head_top">
                <div class="logo">
                    <h1><a href="~/Menu/Home">Katiau<img src="~/images/Logos/katiaulogo.png" width="50" height="50" /></a></h1>
                </div>
                <div class="top-menu">
                    <div class="content white">
                        <nav class="navbar navbar-default">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>
                            <!--/navbar header-->
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                <ul class="nav navbar-nav">
                                    <li><a href="~/Menu/Home">Home</a></li>
                                    <li><a href="~/Menu/Equipe">Sobre</a></li>
                                    <li class="active">
                                        <a href="#" class="scroll dropdown-toggle" data-toggle="dropdown">Análises<b class="caret"></b></a>
                                        <ul class="dropdown-menu">
                                            <li><a href="~/Posts/Posts">Posts</a></li>
                                            <li><a href="~/Reviews/Reviews">Review PC World</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="~/Menu/Galeria">Galeria</a></li>
                                    <li><a href="~/Loja/Principal">Loja</a></li>
                                    <li><a href="~/Menu/Contato">Contato</a></li>

                                    @if (ViewBag.Logado == null)
                                    {
                                        <li class="imagemUser">
                                            <span class="loginform" id="loginform">
                                                <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
                                                <script src="~/JS/index.js"></script>
                                                <img class="login-sign-image" src="~/images/usuarioHover.png"
                                                     onmouseover="this.src='/images/usuario.png'"
                                                     onmouseout="this.src='/images/usuarioHover.png'" />
                                                <div class="login">
                                                    <div class="arrow-up"></div>
                                                    <div class="formholder" style="display: block;">
                                                        <div class="randompad">
                                                            <form method="post" action="~/Menu/Home">
                                                                <e class="msg-erro-login">@ViewBag.MsgErro</e>
                                                                <label class="email" name="email">Email</label>
                                                                <input type="email" id="email" name="email" />
                                                                <label class="senha" name="password">Password</label>
                                                                <input type="password" id="senha" name="senha" />
                                                                <input type="submit" value="Login" />
                                                                <h4 class="cadastrar">Não possui uma conta? <a href="~/Cadastro/Index"><strong>Cadastre-se </strong></a></h4>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                            </span>
                                        </li>
                                    }
                                    @if (ViewBag.NivelAcesso == true)
                                    {
                                        <li><a href="~/Adm2/Listar">ADM</a></li>
                                    }
                                    @if (ViewBag.Logado != null)
                                    {
                                        <li class="imagemUser">
                                            <span class="loginform" id="loginform">
                                                <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
                                                <script src="~/JS/index.js"></script>
                                                <img class="login-sign-image" src="~/images/usuarioHover.png"
                                                     onmouseover="this.src='/images/usuario.png'"
                                                     onmouseout="this.src='/images/usuarioHover.png'" />

                                                <div class="login">
                                                    <div class="arrow-up"></div>
                                                    <div class="formholder" style="display: block;">
                                                        <div class="randompad">
                                                            <a href="~/Perfil/Index"><img class="imagem-logado" src="~/images/img_users/@ViewBag.ImagemU" /></a>
                                                            <p class="login-user-nome">@ViewBag.NomeU <e class="nick-login"><strong>"@ViewBag.NickU"</strong></e><e class="login-user-info"> @ViewBag.SobrenomeU</e></p>
                                                            <a class="deslogar-bt" href="~/Menu/Sair"><strong>Sair</strong></a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </span>
                                        </li>
                                    }
                                </ul>
                            </div>
                        </nav>
                        <!--/navbar-->
                    </div>
                    <div class="clearfix"></div>
                    <script type="text/javascript" src="~/JS/bootstrap-3.1.1.min.js"></script>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <!---->
    <div class="container">
        <div class="single">
            <div class="blog-to">
                
                <div class="banner-post" style="background-image: url(/images/img_posts/@ViewBag.Posts.Imagem)">
                </div>
                <div class="blog-top">
                    <div class="blog-left">
                        <b>@ViewBag.Posts.dia</b>
                        <span>@ViewBag.Posts.mes</span>
                    </div>
                    <div class="top-blog">
                        <a class="fast" > alt+Tab | @ViewBag.Posts.Titulo </a>
                        <p>Postado por <a>@ViewBag.Posts.AutorSobrenome</a>
                        <p class="sed">@ViewBag.Posts.Texto</p>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="clearfix"> </div>
                </div>
            </div>

    </div>
   
        <div class="comment" id="HCB_comment_box">
        <h3>Comentários</h3>
        <style type="text/css">
            #HCB_comment_box #HCB_comment_form_box {
                padding-bottom: 1em;
            }

            #HCB_comment_box .hcb-link {
                cursor: pointer;
            }

            #HCB_comment_box .hcb-icon {
                border: 0px transparent none;
               
            }

            #HCB_comment_box textarea {
                display: block;
                width: 100%;
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                box-sizing: border-box;
                width: 100%;
                         
            }

            #HCB_comment_box blockquote {
                margin: 10px;
                overflow: hidden;
            }

            #HCB_comment_box .hcb-err {
                color: black;
            }

            #HCB_comment_box .hcb-comment-tb {
                margin: 0;
                color: black;
            }

            #HCB_comment_box .comment {
                position: relative;
                color: black;
            }

                #HCB_comment_box .comment .likes {
                    position: absolute;
                    top: 0;
                    right: 0;
                    opacity: 0.8;
                }

                #HCB_comment_box .comment .hcb-comment-tb a {
                    visibility: hidden;
                }

                #HCB_comment_box .comment:hover .hcb-comment-tb a {
                    visibility: visible;
                }

            #HCB_comment_box .gravatar {
                padding-right: 2px;
            }

            #HCB_comment_box input {
                margin-left: 0;
            }
            #HCB_comment_box textarea{
                 color: #B01030;
             }

        </style></div>

